{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-var-requires": false
  }
}
