{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2016",
      "dom"
    ],
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictFunctionTypes": false
  },
}
